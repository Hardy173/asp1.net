
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8"/>
    <title>Details</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.SurName</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
            <td>@item.City</td>
            <td>@item.Experience</td>
            <td>@item.DOB</td>
            <td>@Html.ActionLink("Назад", "Index")</td>
        </tr>
        }
    </table>
</body>
</html>
